# Database Configuration
# For local development (Supabase local)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@127.0.0.1:54322/postgres

# For production (Supabase cloud) - replace with your actual connection string
# DATABASE_URL=postgresql+asyncpg://postgres:[password]@[host]:[port]/[database]

# Database settings
DATABASE_ECHO=false

# Supabase Configuration
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_JWT_SECRET=your_jwt_secret_here

# FastAPI Configuration
ENVIRONMENT=development
DEBUG=true

# CORS Configuration
ADDITIONAL_CORS_ORIGINS=

# FastF1 Configuration (for worker service)
FASTF1_CACHE_DIR=./cache/fastf1
